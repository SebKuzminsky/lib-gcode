
o<presentation-position-0> sub
    ; switch to absolute motion mode
    g90

    ; cancel cutter comp
    g40

    ; quill up
    g53 g0 z0

    ; spindle off
    m5

    ; rapid to presentation position
    ; table centered in X, all the way forward towards the user
    g53 g0 x9 y12
o<presentation-position-0> endsub

m2

